- provide(:title, t('.title'))

.container#software-index
  %h1
    开发工具下载
  .row
    - @softwares.each do |s|
      .col-md-3
        .card
          .software-title
            = s.name
          .software-download
            = link_to "下载", software_path(s), :class => "btn-dark-brd btn-base-sm radius-3", :target => "_blank"
